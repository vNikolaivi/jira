<div class="task priority_<%= task.priority %> completed_ <%= task.completed %>">
  <table>
  <thead>
    <tr>
      <th>Project #</th>
      <th>Title</th>
      <th>Description</th>
      <th>Notes</th>
      <th>Autor</th>
      <th>Executor</th>
      <th>Priority</th>
      <th>Status</th>
      <th>Estimation Time</th>
      <th>Completed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
<tr>
        <td><%= task.project_id %></td>
	<td><%= task.title %></td>
	<td><%= task.description %></td>
	<td><%= task.notes %></td>
	<td><%= task.autor %></td>
	<td><%= task.executor %></td>
	<td><%= task.priority %></td>
	<td><%= task.status %></td>
	<td><%= task.estimation_time %></td>
	<td><%= task.completed %></td>
        <td><%= link_to 'Show', task_path(task) %></td>
        <td><%= link_to 'Edit', edit_task_path(task) %></td>
        <td><%= link_to 'Destroy', task, method: :delete, data: { confirm: 'Are you sure?' } %>
</td>
 </tr>
    
  </tbody>
</table>     
  
   
</div>
