
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <h2>Log in</h2>
<%= render "devise/shared/error_messages", resource: resource %>

<div class="form-group">
      <%- if devise_mapping.registerable? %>


      <%= f.label :email %><br/>
      <%= f.email_field :email, autofocus: true, class: "form-control", autocomplete: "email" %>
  <% end -%>
  </div>

  <div class="form-group">
    <%- if devise_mapping.recoverable? %>

      <%= f.label :password %><br/>
      <%= f.password_field :password, class: "form-control", autocomplete: "current-password" %>
    <% end -%>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="form-group">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in", class: "btn-primary" %>
  </div>
  <% end %>
 </div>
</div>
</div>
<%= render "devise/shared/links" %>
<% controller.redirect_to_root_path%>
